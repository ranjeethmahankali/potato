{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "potato-debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Debug/potato",
      "args": [
        ""
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "dap-compilation": "${workspaceFolder}/build.sh Debug",
      "dap-compilation-dir": "${workspaceFolder}"
